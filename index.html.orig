<!-- Build version  <%= htmlWebpackPlugin.options.build %> -->
<!DOCTYPE html>
<html class="darkblue">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no, minimal-ui" />
        <meta name="HandheldFriendly" content="true" />
        <meta name="mobile-web-app-capable" content="yes" />
        <title>MARBEFES BBT Database</title>
        <link rel="stylesheet" type="text/css" href="assets/css/qwc2.css" />
        <link rel="stylesheet" type="text/css" href="assets/css/colorschemes.css" />
        <link rel="stylesheet" type="text/css" href="assets/css/custom-buttons.css" />
        <script src="debug.js"></script>
        <script>
            console.log('=== INLINE SCRIPT EXECUTING ===');
            document.addEventListener('DOMContentLoaded', function() {
                console.log('=== DOM CONTENT LOADED ===');
                
                // Create a visible indicator
                const indicator = document.createElement('div');
                indicator.id = 'js-indicator';
                indicator.style.cssText = 'position: fixed; top: 10px; right: 10px; background: red; color: white; padding: 10px; z-index: 99999; font-size: 14px;';
                indicator.textContent = 'JavaScript is working!';
                document.body.appendChild(indicator);
                
                // Log button information
                setTimeout(() => {
                    const buttons = document.querySelectorAll('button');
                    console.log('=== FOUND BUTTONS ===', buttons.length);
                    
                    // Add click handlers to all buttons
                    buttons.forEach((btn, i) => {
                        console.log(`Button ${i}:`, btn.textContent?.trim(), btn.title);
                        btn.addEventListener('click', (e) => {
                            console.log('=== BUTTON CLICKED ===', btn.title || btn.textContent);
                            indicator.textContent = `Clicked: ${btn.title || btn.textContent}`;
                            indicator.style.background = 'green';
                        });
                    });
                }, 3000);
            });
        </script>
        <link rel="apple-touch-icon" href="assets/img/app_icon.png"/>
        <link rel="apple-touch-icon" sizes="72x72" href="assets/img/app_icon_72.png"/>
        <link rel="apple-touch-icon" sizes="114x114" href="assets/img/app_icon_114.png"/>
        <link rel="apple-touch-icon" sizes="144x144" href="assets/img/app_icon_144.png"/>
        <link rel="icon" href="assets/img/marbefes-logo.svg" type="image/svg+xml" />
        <link rel="alternate icon" href="assets/img/favicon.ico" />
        <style type="text/css">
        div#splash {
            position: fixed;
            left: 0;
            right: 0;
            top: 0;
            bottom: 0;
            display: flex;
            align-items: center;
            justify-content: center;
        }
        .lds-dual-ring {
            display: inline-block;
            width: 80px;
            height: 80px;
        }
        .lds-dual-ring:after {
            content: " ";
            display: block;
            width: 64px;
            height: 64px;
            margin: 8px;
            border-radius: 50%;
            border: 6px solid #595959;
            border-color: #595959 transparent #595959 transparent;
            animation: lds-dual-ring 1.2s linear infinite;
        }
        @keyframes lds-dual-ring {
            0% {
                transform: rotate(0deg);
            }
            100% {
                transform: rotate(360deg);
            }
        }
        
        /* FORCE BUTTON POSITIONING - Data and Bowtie buttons - ULTRA AGGRESSIVE */
        
        /* Target by position - force last buttons to left side */
        .bowtie-analysis-button,
        .data-portal-button,
        html body .bowtie-analysis-button,
        html body .data-portal-button,
        html body button.bowtie-analysis-button,
        html body button.data-portal-button {
            position: fixed !important;
            left: 10px !important;
            z-index: 9999 !important;
            right: auto !important;
            width: 40px !important;
            height: 40px !important;
            border-radius: 6px !important;
            color: white !important;
            transform: none !important;
            margin: 0 !important;
            padding: 0 !important;
        }
        
        
        /* Alternative targeting by any attribute containing our keywords - MAXIMUM SPECIFICITY */
        .bowtie-analysis-button,
        html body .bowtie-analysis-button,
        html body button.bowtie-analysis-button,
        html body .toolbar .bowtie-analysis-button,
        button[class*="bowtie-analysis-button"],
        *[class*="bowtie-analysis-button"] {
            position: fixed !important;
            left: 10px !important;
            top: 160px !important;
            background-color: #4CAF50 !important;
            background: #4CAF50 !important;
            border: 2px solid #388E3C !important;
            border-color: #388E3C !important;
            color: white !important;
            box-shadow: 0 2px 8px rgba(76, 175, 80, 0.3) !important;
            border-radius: 6px !important;
            width: 40px !important;
            height: 40px !important;
            z-index: 9999 !important;
            right: auto !important;
            transform: none !important;
            margin: 0 !important;
            padding: 0 !important;
        }
        
         .data-portal-button,
         html body .data-portal-button,
         html body button.data-portal-button,
         html body .toolbar .data-portal-button,
         button[class*="data-portal-button"],
         *[class*="data-portal-button"] {
            position: fixed !important;
            left: 10px !important;
            top: 100px !important;
            background-color: #4CAF50 !important;
            background: #4CAF50 !important;
            border: 2px solid #388E3C !important;
            border-color: #388E3C !important;
            color: white !important;
            box-shadow: 0 2px 8px rgba(76, 175, 80, 0.3) !important;
            border-radius: 6px !important;
            width: 40px !important;
            height: 40px !important;
            z-index: 9999 !important;
            right: auto !important;
            transform: none !important;
            margin: 0 !important;
            padding: 0 !important;
        }
        </style>
        <script type="text/javascript" src="assets/searchProviders.js" ></script>
        <script type="text/javascript">
        window.addEventListener("load", function(ev) {
            document.getElementById('splash').style.display = 'none';
        }, false);
        </script>
    </head>
    <body>
        <div id="splash"><div class="lds-dual-ring"></div></div>
        <div id="container"></div>
    </body>

</html>
